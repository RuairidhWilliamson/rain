let print = internal._print

fn _import(path) {
	internal._import(internal._get_file(path))
}

let a = 5
let b = a
let foo = _import("foo.rain")
let bar = _import("bar.rain")

fn main() {
	print("foo.apples:", foo.apples)
	print("foo.bananas:", foo.bananas)
	print("bar.apples:", bar.apples)
	print("bar.bananas:", bar.bananas)
}
