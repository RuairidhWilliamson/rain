let area = internal._get_area(internal._module_file())
let main = internal._import(internal._get_file(area, "main.rain"))
let std = main.std

fn build() {
	channel = std.build.rust.stable
	src = std.utils.get_file("main.rs")
	outname = "rust-helloworld"
	target = "x86_64-unknown-linux-musl"
	edition = "2024"
	std.build.rust.rustc.compile(channel, src, outname, target, edition)
}
