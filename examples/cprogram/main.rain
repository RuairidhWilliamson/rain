fn build() {
    let clang = std.escape.bin("clang")
    let in = std.path("./main.c")
    let out = std.path("./foo")
    let build = std.run(clang, in, "-o", out)
    # let output_file = build.output_file(out)
    # return output_file.as_executable()
    return out
}

fn run() {
    let out = build()
    std.run(out)
}

run()
