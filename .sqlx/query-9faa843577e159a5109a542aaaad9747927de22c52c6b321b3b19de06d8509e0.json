{
  "db_name": "PostgreSQL",
  "query": "SELECT\n    runs.id,\n    commit,\n    created_at,\n    dequeued_at,\n    rain_version,\n    repos.id as repo_id,\n    repos.host,\n    repos.owner,\n    repos.name,\n    target,\n    finished_at as \"finished_at?\",\n    status as \"status?\",\n    execution_time_millis as \"execution_time_millis?\",\n    output as \"output?\"\nFROM runs\nINNER JOIN repos ON runs.repo=repos.id\nLEFT OUTER JOIN finished_runs ON runs.id=finished_runs.run\nWHERE repos.id=$1\nORDER BY runs.id DESC\nOFFSET $2 LIMIT $3;\n",
