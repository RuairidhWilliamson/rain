let print = core.print

let cargo = std.escape.bin("cargo")

fn check() {
	print(cargo)
	let out = std.run(cargo, "clippy", "--workspace", "--", "--deny", "warnings")
	if out.success {
		print("Success")
	} else {
		print("Failed")
	}

	# std.run(cargo, "fmt", "--check")
	# std.run(cargo, "test", "--workspace")
	# std.run(cargo, "outdated", "--workspace")
}

check()
